@foreach (ClothMarketApplication.Models.UserProfile e in @ViewBag.viewUsers)
{

    <div class="container">

        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Password</th>
                    <th>Type</th>
                    <th>Picture</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td><h5> @e.Id </h5></td>
                    <td><h5>@e.name </h5></td>
                    <td><h5> @e.email</h5></td>
                    <td><h5> @e.password </h5></td>
                    <td><h5> @e.type</h5></td>
                    <td><h5><img src="~/Files/@e.profilePic" height="70" width="80" /></h5></td>
                </tr>
